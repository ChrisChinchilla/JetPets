                    ->entityCondition('bundle', 'survey_response')
                    ->propertyCondition('status', 1)
                    ->fieldCondition('field_gf_id', 'value', $value['id'], '=')
                    ->range(0, 1)
                    ->addMetaData('account', user_load(1)); // Run the query as user 1.

                $result = $query->execute();

